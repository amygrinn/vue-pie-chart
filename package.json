{
  "name": "@tygr/vue-pie-chart",
  "version": "1.0.5",
  "description": "Pie chart",
  "main": "dist/vue-pie-chart.js",
  "files": [
    "src/index.d.ts",
    "dist"
  ],
  "types": "src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylergrinn/vue-pie-chart.git"
  },
  "scripts": {
    "start": "webpack && run-p build:w test:w",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack",
    "test": "run-s unit e2e",
    "test:w": "run-p unit:w e2e:w",
    "build:w": "cross-env NODE_ENV=development webpack -w",
    "unit": "mocha -r @babel/register -r jsdom-global/register tests/pie-chart.spec.js",
    "unit:w": "yarn unit -w",
    "e2e": "concurrently -k -s first \"http-server -s -p 8000 dist\" \"cypress run\"",
    "e2e:w": "concurrently \"http-server -s -p 8000 dist\" \"cypress open\""
  },
  "keywords": [
    "pie",
    "chart",
    "vue",
    "mobile",
    "desktop"
  ],
  "author": "Tyler J Grinn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tylergrinn/vue-pie-chart/issues"
  },
  "homepage": "https://github.com/tylergrinn/vue-pie-chart#readme",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.5",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cypress": "^3.1.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jsdom": "^13.2.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.0.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333",
    "vue": "^2.6.7",
    "vue-loader": "^15.6.4",
    "vue-template-compiler": "^2.6.7",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "vue": "2"
  }
}
